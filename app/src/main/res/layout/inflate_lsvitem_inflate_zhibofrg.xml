<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_inflate_lsvitem_zhibofrg"
    android:layout_width="match_parent"
    android:layout_height="350pt"
    android:paddingEnd="26pt"
    android:paddingStart="26pt">

    <TextView
        android:id="@+id/tv_inflate_lsvitem_zhibofrg_banji"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="29pt"
        android:text=""
        android:textColor="@color/color_323232"
        android:textSize="36pt" />

    <TextView
        android:id="@+id/tv_inflate_lsvitem_zhibofrg_kechengzhaiyao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv_inflate_lsvitem_zhibofrg_banji"
        android:layout_below="@+id/tv_inflate_lsvitem_zhibofrg_banji"
        android:layout_marginTop="14pt"
        android:text=""
        android:textColor="@color/color_646464"
        android:textSize="28pt" />

    <ImageView
        android:id="@+id/textView7"
        android:layout_width="24pt"
        android:layout_height="24pt"
        android:layout_alignStart="@+id/tv_inflate_lsvitem_zhibofrg_kechengzhaiyao"
        android:layout_below="@+id/tv_inflate_lsvitem_zhibofrg_kechengzhaiyao"
        android:layout_marginTop="25pt"
        android:src="@drawable/timeyellow_icon_disable" />

    <TextView
        android:id="@+id/tv_inflate_lsvitem_zhibofrg_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_marginStart="26pt"
        android:layout_toEndOf="@+id/textView7"
        android:gravity="bottom"
        android:text=""
        android:textColor="@color/color_909090"
        android:textSize="24pt" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:layout_marginBottom="35pt"
        android:layout_marginTop="42pt"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0pt"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_inflate_lsvitem_zhibofrg_pic1"
                    android:layout_width="70pt"
                    android:layout_height="70pt"
                    android:scaleType="fitXY"
                    android:src="@drawable/touxiang_image_zhiboshouye"
                    android:visibility="invisible" />

                <TextView
                    android:id="@+id/tv_inflate_lsvitem_zhibofrg_name1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12pt"
                    android:text="林克我"
                    android:textColor="@color/color_646464"
                    android:textSize="24pt"
                    android:visibility="invisible" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="30pt"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_inflate_lsvitem_zhibofrg_pic2"
                    android:layout_width="70pt"
                    android:layout_height="70pt"
                    android:scaleType="fitXY"
                    android:src="@drawable/touxiang_image_zhiboshouye"
                    android:visibility="invisible" />

                <TextView
                    android:id="@+id/tv_inflate_lsvitem_zhibofrg_name2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12pt"
                    android:text="林克我"
                    android:textColor="@color/color_646464"
                    android:textSize="24pt"
                    android:visibility="invisible" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="30pt"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/img_inflate_lsvitem_zhibofrg_pic3"
                    android:layout_width="70pt"
                    android:layout_height="70pt"
                    android:scaleType="fitXY"
                    android:src="@drawable/touxiang_image_zhiboshouye"
                    android:visibility="invisible" />

                <TextView
                    android:id="@+id/tv_inflate_lsvitem_zhibofrg_name3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12pt"
                    android:text="林克我"
                    android:textColor="@color/color_646464"
                    android:textSize="24pt"
                    android:visibility="invisible" />
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/rl_inflate_lsvitem_zhibofrg_price"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="￥"
                android:textColor="@color/color_08D2B2"
                android:textSize="48pt" />

            <TextView
                android:id="@+id/tv_inflate_lsvitem_zhibofrg_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/tv"
                android:text=""
                android:textColor="@color/color_08D2B2"
                android:textSize="48pt" />

            <TextView
                android:id="@+id/tv2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv"
                android:text="已有"
                android:textColor="@color/color_909090"
                android:textSize="24pt" />

            <TextView
                android:id="@+id/tv_inflate_lsvitem_zhibofrg_pn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/tv2"
                android:layout_toEndOf="@+id/tv2"
                android:text=""
                android:textColor="@color/color_FF9B19"
                android:textSize="30pt" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/tv2"
                android:layout_toEndOf="@+id/tv_inflate_lsvitem_zhibofrg_pn"
                android:text="人购买"
                android:textColor="@color/color_909090"
                android:textSize="24pt" />

        </RelativeLayout>
    </LinearLayout>


</RelativeLayout>
